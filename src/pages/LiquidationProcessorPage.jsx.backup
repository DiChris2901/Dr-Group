    columnMapping['Tipo apuesta'] = typeCol;
    console.log(`â Columna Tipo apuesta encontrada: ${typeCol}`);
  } else {
    console.log('â ï¸ No se encontrÃ³ columna Tipo apuesta, usando valor por defecto');
  }

  // Base liquidaciÃ³n
  const baseCol = findColumn(liquidationData, ['base liquidacion diaria', 'base liquidacion', 'base', 'produccion', 'ingresos']);
  if (baseCol) {
    columnMapping['Base liquidaciÃ³n diaria'] = baseCol;
    console.log(`â Columna Base liquidaciÃ³n encontrada: ${baseCol}`);
  } else {
    throw new Error('â No se encontrÃ³ columna de Base liquidaciÃ³n en el archivo');
  }

  // Fecha reporte
  const dateCol = findColumn(liquidationData, ['fecha reporte', 'fecha', 'periodo']);
  if (dateCol) {
    columnMapping['Fecha reporte'] = dateCol;
    console.log(`â Columna Fecha reporte encontrada: ${dateCol}`);
  } else {
    console.log('â ï¸ No se encontrÃ³ columna Fecha reporte');
  }

  console.log('ð§ Consolidando datos por Serial...');

  // Agrupar y consolidar por Serial
  const groupedData = {};
  
  liquidationData.forEach((row, index) => {
    const serial = row[columnMapping['Serial']]?.toString().trim();
    if (!serial) {
      console.log(`â ï¸ Fila ${index + 1} sin Serial, omitiendo`);
      return;
    }

    if (!groupedData[serial]) {
      groupedData[serial] = {
        serial,
        nuc: row[columnMapping['NUC']]?.toString().trim() || '',
        establecimiento: row[columnMapping['Establecimiento']]?.toString().trim() || '',
        tipo: row[columnMapping['Tipo apuesta']]?.toString().trim() || 'General',
        fechas: [],
        totalBase: 0
      };
    }

    // Agregar fecha y base
    const fecha = row[columnMapping['Fecha reporte']];
    const base = Number(row[columnMapping['Base liquidaciÃ³n diaria']]) || 0;
    
    if (fecha) {
      groupedData[serial].fechas.push(fecha);
    }
    
    groupedData[serial].totalBase += base;
  });

  console.log(`ð ConsolidaciÃ³n completada: ${Object.keys(groupedData).length} mÃ¡quinas Ãºnicas`);

  // Convertir a array y calcular campos derivados
  const consolidatedData = Object.values(groupedData).map(group => {
    // Calcular fechas
    const fechasValidas = group.fechas.filter(f => f);
    const primerDia = fechasValidas.length > 0 ? new Date(Math.min(...fechasValidas.map(f => new Date(f)))) : null;
    const ultimoDia = fechasValidas.length > 0 ? new Date(Math.max(...fechasValidas.map(f => new Date(f)))) : null;
    const diasTransmitidos = fechasValidas.length;

    // Calcular dÃ­as del mes
    let diasDelMes = 31;
    if (ultimoDia) {
      const year = ultimoDia.getFullYear();
      const month = ultimoDia.getMonth();
      diasDelMes = new Date(year, month + 1, 0).getDate();
    }

    // Determinar novedad
    let novedad = 'Sin informaciÃ³n';
    if (diasTransmitidos === diasDelMes) {
      novedad = 'Sin cambios';
    } else if (diasTransmitidos < diasDelMes) {
      novedad = 'Retiro / AdiciÃ³n';
    } else {
      novedad = 'DÃ­as excedentes';
    }

    // Crear perÃ­odo texto
    const periodoTexto = ultimoDia ? convertDateToPeriod(ultimoDia) : '';

    // Calcular impuestos segÃºn fÃ³rmulas correctas del Python
    const produccion = group.totalBase;
    const derechosExplotacion = produccion * 0.12; // 12%
    const gastosAdministracion = derechosExplotacion * 0.01; // 1% de los derechos
    const totalImpuestos = derechosExplotacion + gastosAdministracion;

    return {
      'Empresa': selectedCompany || 'No especificada',
      'Serial': group.serial,
      'NUC': group.nuc,
      'Establecimiento': group.establecimiento,
      'DÃ­as transmitidos': diasTransmitidos,
      'DÃ­as del mes': diasDelMes,
      'Primer dÃ­a transmitido': primerDia ? primerDia.toLocaleDateString('es-ES') : '',
      'Ãltimo dÃ­a transmitido': ultimoDia ? ultimoDia.toLocaleDateString('es-ES') : '',
      'PerÃ­odo Texto': periodoTexto,
      'Tipo apuesta': group.tipo,
      'ProducciÃ³n': produccion,
      'Derechos de ExplotaciÃ³n': derechosExplotacion,
      'Gastos de AdministraciÃ³n': gastosAdministracion,
      'Total Impuestos': totalImpuestos,
      'Novedad': novedad
    };
  });

  return consolidatedData;
};

// FunciÃ³n para generar reporte por sala/establecimiento
const generateRoomReport = (consolidatedData) => {
  console.log('ð¢ Generando reporte por establecimiento...');
  
  if (!consolidatedData.length) {
    console.log('â No hay datos consolidados para generar reporte por sala');
    return [];
  }

  // Agrupar por establecimiento
  const roomData = {};

  consolidatedData.forEach(row => {
    const establishment = row.Establecimiento;
    if (!establishment || establishment === 'No encontrado') return;

    if (!roomData[establishment]) {
      roomData[establishment] = {
        establecimiento: establishment,
        empresa: row.Empresa,
        totalMaquinas: 0,
        produccionTotal: 0,
        derechosTotal: 0,
        gastosTotal: 0,
        impuestosTotal: 0
      };
    }

    roomData[establishment].totalMaquinas += 1;
    roomData[establishment].produccionTotal += row['ProducciÃ³n'] || 0;
    roomData[establishment].derechosTotal += row['Derechos de ExplotaciÃ³n'] || 0;
    roomData[establishment].gastosTotal += row['Gastos de AdministraciÃ³n'] || 0;
    roomData[establishment].impuestosTotal += row['Total Impuestos'] || 0;
  });

  // Convertir a array y calcular promedios
  const roomReport = Object.values(roomData).map(room => ({
    'Empresa': room.empresa,
    'Establecimiento': room.establecimiento,
    'Total MÃ¡quinas': room.totalMaquinas,
    'ProducciÃ³n': room.produccionTotal,
    'Derechos de ExplotaciÃ³n': room.derechosTotal,
    'Gastos de AdministraciÃ³n': room.gastosTotal,
    'Total Impuestos': room.impuestosTotal,
    'Promedio/Establecimiento': room.totalMaquinas > 0 ? room.produccionTotal / room.totalMaquinas : 0
  }));

  // Ordenar por producciÃ³n total descendente
  roomReport.sort((a, b) => b['ProducciÃ³n'] - a['ProducciÃ³n']);

  console.log(`â Reporte por establecimiento completado: ${roomReport.length} establecimientos`);
  return roomReport;
};

// FunciÃ³n para convertir fecha a perÃ­odo texto
const convertDateToPeriod = (date) => {
  try {
    const d = new Date(date);
    const month = (d.getMonth() + 1).toString().padStart(2, '0');
    const year = d.getFullYear();
    
    const months = {
      '01': 'Enero', '02': 'Febrero', '03': 'Marzo', '04': 'Abril',
      '05': 'Mayo', '06': 'Junio', '07': 'Julio', '08': 'Agosto',
      '09': 'Septiembre', '10': 'Octubre', '11': 'Noviembre', '12': 'Diciembre'
    };
    
    return `${months[month]} ${year}`;
  } catch (error) {
    console.log('â ï¸ Error convirtiendo fecha a perÃ­odo:', error);
    return '';
  }
};

// FunciÃ³n para detectar fila de encabezados automÃ¡ticamente
const detectHeaderRow = (data) => {
  if (!data || data.length === 0) return 0;

  const keyColumns = ['serial', 'nuc', 'nuid', 'establecimiento'];
  
  for (let rowIndex = 0; rowIndex < Math.min(data.length, 10); rowIndex++) {
    const row = data[rowIndex];
    const rowValues = Object.values(row).map(val => 
      val?.toString().toLowerCase().trim() || ''
    );
    
    let matches = 0;
    keyColumns.forEach(key => {
      if (rowValues.some(val => val.includes(key))) {
        matches++;
      }
    });
    
    if (matches >= 2) {
      console.log(`ð Encabezados detectados en fila ${rowIndex + 1} con ${matches} coincidencias`);
      return rowIndex;
    }
  }

  console.log('â ï¸ No se detectaron encabezados automÃ¡ticamente, usando fila 1');
  return 0;
};

// FunciÃ³n para extraer nÃºmero de contrato del archivo de liquidaciÃ³n
const extractContractNumber = (liquidationData) => {
  console.log('ð Extrayendo nÃºmero de contrato del archivo...');
  
  if (!liquidationData || liquidationData.length === 0) {
    console.log('â No hay datos para extraer nÃºmero de contrato');
    return null;
  }

  // Buscar en las primeras filas (pueden estar en metadata al inicio)
  for (let i = 0; i < Math.min(10, liquidationData.length); i++) {
    const row = liquidationData[i];
    
    // Buscar en todas las columnas de la fila
    for (const [key, value] of Object.entries(row)) {
      if (value && typeof value === 'string') {
        // Buscar patrones de nÃºmero de contrato (tÃ­picamente alfanumÃ©rico)
        // Ejemplo: "Contrato: ABC123", "Contract: 123456", o simplemente "CJ0R06"
        const contractPatterns = [
          /contrato[:\s]*([A-Z0-9]+)/i,
          /contract[:\s]*([A-Z0-9]+)/i,
          /^[A-Z]{1,3}\d{3,6}$/,  // PatrÃ³n comÃºn: CJ0R06, ABC123, etc.
          /^[A-Z0-9]{4,10}$/      // CÃ³digo alfanumÃ©rico de 4-10 caracteres
        ];
        
        for (const pattern of contractPatterns) {
          const match = value.match(pattern);
          if (match) {
            const contractNumber = match[1] || match[0];
            console.log(`â NÃºmero de contrato encontrado: ${contractNumber} (en columna: ${key})`);
            return contractNumber;
          }
        }
      }
    }
  }
  
  // Si no se encuentra en metadata, buscar en datos (primera columna que parezca nÃºmero de contrato)
  if (liquidationData.length > 0) {
    const firstDataRow = liquidationData[0];
    for (const [key, value] of Object.entries(firstDataRow)) {
      if (value && typeof value === 'string' && /^[A-Z0-9]{4,10}$/i.test(value)) {
        console.log(`â Posible nÃºmero de contrato encontrado en datos: ${value} (columna: ${key})`);
        return value;
      }
    }
  }
  
  console.log('â ï¸ No se pudo extraer nÃºmero de contrato automÃ¡ticamente');
  return null;
};

// FunciÃ³n para buscar empresa por nÃºmero de contrato
const findCompanyByContractNumber = (companies, contractNumber) => {
  console.log(`ð Buscando empresa con nÃºmero de contrato: ${contractNumber}`);
  
