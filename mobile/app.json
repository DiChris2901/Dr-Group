{
  "expo": {
    "name": "DR Group Asistencia",
    "slug": "drgroup-mobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drgroup.asistencia"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.drgroup.asistencia",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "NOTIFICATIONS",
        "VIBRATE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "edgeToEdgeEnabled": true,
      "useNextNotificationsApi": true
    },
    "updates": {
      "url": "https://u.expo.dev/169f6749-ebbd-4386-9359-b60f7afe299d",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-notifications",
      "expo-secure-store",
      "expo-updates",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "169f6749-ebbd-4386-9359-b60f7afe299d"
      }
    }
  }
}
